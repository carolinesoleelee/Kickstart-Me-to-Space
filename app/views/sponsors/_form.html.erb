<%= form_for(form_object) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>
  <%= f.label :net_worth %>
  <%= f.number_field :net_worth%><br>
  <%= f.label :goal %>
  <%= f.text_area :goal %><br>

  <%= f.submit %>
<% end %>
