<%= form_for(@sponsor) do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>
  <%= f.label :net_worth %>
  <%= f.number_field :net_worth%><br>
  <%= f.label :goal %>
  <%= f.text_area :goal %><br>

  <%= f.submit %>
<% end %>
